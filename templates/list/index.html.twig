{% extends 'base.html.twig' %}

{% block title %}Hello ListController!{% endblock %}

{% block body %}
    {% for announcement in announcements %}
        <br/> Announcement :<br/>
        Id = {{ announcement.id }}<br/>
        Title =  {{ announcement.title }}<br/>
        Content = {{ announcement.getContent() }}<br/>
        Price = {{ announcement.getPrice() }}<br/>
        Created Date = {{ announcement.getCreatedAt() | date('d/m/Y') }}<br/>
    {% endfor %}
{% endblock %}
